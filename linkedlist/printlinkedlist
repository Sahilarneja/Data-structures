public class Node{
    int data;
    Node Next;

    Node(int data){
        this.data=data;
        this.Next=null;

    }

    public static void main(String []args){
        Node n1=new Node(10);
        Node n2=new Node(20);
        n1.Next=n2;
        System.out.println(n1.data);
        System.out.println(n1.Next);
        System.out.println(n2.data);
        System.out.println(n2.Next);
    }
}